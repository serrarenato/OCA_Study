package br.com.java.arrays;

import java.util.ArrayList;
import java.util.List;
import java.util.Scanner;



public class CountStrings2 {
	public static void main(String args[]) {
		Scanner sc = new Scanner(System.in);
		int T = sc.nextInt();
		List<String> list = new ArrayList<>();
		while (T-- > 0) {
			list.add(sc.next());			
		}
		int Q = sc.nextInt();
		List<String> listSeek = new ArrayList<>();
		while (Q-- > 0) {
			listSeek.add(sc.next());			
		}
		
		for (String string : listSeek) {
			System.out.println(seekinArray(string, list));
		}
	}

	private static int seekinArray(String string, List<String> list) {
		int number=0;
		for (String string2 : list) {
			if (string2.equals(string))
				number++;
			
		}
		return number;
	}
}
