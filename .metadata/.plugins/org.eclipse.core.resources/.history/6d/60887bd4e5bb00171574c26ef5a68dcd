package br.com.java.arrays;

/*
 * With HashMap instead of list
 */
import java.util.ArrayList;
import java.util.HashMap;
import java.util.List;
import java.util.Map;
import java.util.Scanner;
import java.util.Set;

public class CountStrings2 {
	public static void main(String args[]) {
		Scanner sc = new Scanner(System.in);
		int T = sc.nextInt();

		Map<String, Integer> list = new HashMap<>();
		while (T-- > 0) {
			String str=sc.next();
			if (list.containsKey(str))
				Integer value = (Integer) list.get(str);
				else
					list.put(str, 1);
		}
		int Q = sc.nextInt();
		List<String> listSeek = new ArrayList<>();
		while (Q-- > 0) {
			listSeek.add(sc.next());
		}

		for (String string : listSeek) {
			System.out.println(seekinArray(string, list));
		}
	}

	private static int seekinArray(String string, Set<String> list) {
		int number = 0;
		for (String string2 : list) {
			if (string2.equals(string))
				number++;

		}
		return number;
	}
}
