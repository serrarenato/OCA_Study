package br.com.java.arrays;

/*
 * With set instead of list
 */
import java.util.ArrayList;
import java.util.HashSet;
import java.util.List;
import java.util.Scanner;
import java.util.Set;

class testeDTO {
	Integer number;
	String string;

	public testeDTO() {
	};

	public testeDTO(String string) {
		this.string = string;

	}

	@Override
	public int hashCode() {
		final int prime = 31;
		int result = 1;
		result = prime * result + ((string == null) ? 0 : string.hashCode());
		return result;
	}

	@Override
	public boolean equals(Object obj) {
		if (this == obj)
			return true;
		if (obj == null)
			return false;
		if (getClass() != obj.getClass())
			return false;
		testeDTO other = (testeDTO) obj;
		if (string == null) {
			if (other.string != null)
				return false;
		} else if (!string.equals(other.string))
			return false;
		return true;
	}

}

public class CountStrings2 {
	public static void main(String args[]) {
		Scanner sc = new Scanner(System.in);
		int T = sc.nextInt();
		Set<testeDTO> list = new HashSet<>();
		while (T-- > 0) {
			if (list.contains(arg0))
				list.add(sc.next());
		}
		int Q = sc.nextInt();
		List<String> listSeek = new ArrayList<>();
		while (Q-- > 0) {
			listSeek.add(sc.next());
		}

		for (String string : listSeek) {
			System.out.println(seekinArray(string, list));
		}
	}

	private static int seekinArray(String string, Set<String> list) {
		int number = 0;
		for (String string2 : list) {
			if (string2.equals(string))
				number++;

		}
		return number;
	}
}
