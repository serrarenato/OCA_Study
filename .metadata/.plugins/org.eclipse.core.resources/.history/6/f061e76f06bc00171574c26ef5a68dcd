package br.com.java.arrays;

import java.util.Scanner;

public class sumArrays {
	public static void sum(int a, int b, int c, int[] array) {
		a--;
		b--;
		for (; a <= b; a++) {
			array[a] += c;
		}
	}

	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		int n = in.nextInt();
		int m = in.nextInt();
		int[] array = new int[n];
		for (int a0 = 0; a0 < m; a0++) {
			int a = in.nextInt();
			int b = in.nextInt();
			int k = in.nextInt();
			sum(a, b, k, array);
			
		}
		System.out.println(array);

	}
}
